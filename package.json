{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "buildNpmdoc": "bunyan",
    "description": "api documentation for [bunyan (v1.8.9)](https://github.com/trentm/node-bunyan#readme) [![travis-ci.org build-status](https://api.travis-ci.org/npmdoc/node-npmdoc-bunyan.svg)](https://travis-ci.org/npmdoc/node-npmdoc-bunyan)",
    "devDependencies": {
        "bunyan": "*",
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "utility2": "kaizhu256/node-utility2#alpha"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmdoc/node-npmdoc-bunyan",
    "keywords": [
        "documentation",
        "bunyan"
    ],
    "license": "MIT",
    "main": "lib.npmdoc_bunyan.js",
    "name": "npmdoc-bunyan",
    "nameAlias": "npmdoc_bunyan",
    "nameOriginal": "npmdoc-bunyan",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmdoc/node-npmdoc-bunyan.git"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "(set -e; npm install \"kaizhu256/node-utility2#alpha\"; utility2 shDeployHeroku)",
        "postinstall": "if [ -f npm_scripts.sh ]; then ./npm_scripts.sh postinstall; fi",
        "start": "(set -e; export PORT=${PORT:-8080}; utility2 start test.js)",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "0.0.2"
}
